 (cd;sget proj-arm64/sample/test/test*acap*)
 (cd;sget proj-arm64/sample/test/*obj)

#####################################################spgemm
 (cd spgemm;                                             ./MAKE_BSD)
 (cd;sput proj-arm64/sample/spgemm/{M*,*.[ch],*obj})
sssh "ssh $HOST_CENT '(cd proj-arm64/sample/spgemm;      ./MAKE_CENT)'"
