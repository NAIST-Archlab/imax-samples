#!/bin/csh

set HOST_CENT=cad101
set HOST_UBUNTU=arch07
set HOST_FGKU=cadpc01
set HOST_ZYNQ=arch28
set HOST_ACAP=alice122

#(cd 4dimage;      ./MAKE_BSD)
#(cd Kdijkstra;    ./MAKE_BSD)
#(cd dijkstra;     ./MAKE_BSD)

#####################################################stanford
 (cd stanford;                                           ./MAKE_BSD)

#####################################################conv16
 (cd conv16;                                             ./MAKE_BSD)
 (cd;sput proj-arm64/sample/conv16/{M*,*.[ch],*obj})

#####################################################filter
 (cd filter;                                             ./MAKE_BSD)
 (cd;sput proj-arm64/sample/filter/{M*,*.[ch],*obj})
sssh "ssh $HOST_CENT '(cd proj-arm64/sample/filter;      ./MAKE_CENT)'"
 (cd;sget proj-arm64/sample/filter/filter-cent*)
sssh "ssh $HOST_ZYNQ '(cd proj-arm64/sample/filter;      ./MAKE_ZYNQ)'"
 (cd;sget proj-arm64/sample/filter/filter-zynq*)
sssh "ssh $HOST_ACAP '(cd proj-arm64/sample/filter;      ./MAKE_ACAP)'"
 (cd;sget proj-arm64/sample/filter/filter-acap*)
 (cd;sget proj-arm64/sample/filter/*obj)

#####################################################mm_cnn_lf
 (cd mm_cnn_lf;                                          ./MAKE_BSD)
 (cd;sput proj-arm64/sample/mm_cnn_lf/{M*,*.[ch],*obj})
sssh "ssh $HOST_CENT '(cd proj-arm64/sample/mm_cnn_lf;   ./MAKE_CENT)'"
 (cd;sget proj-arm64/sample/mm_cnn_lf/[a-z]*cent*)
sssh "ssh $HOST_ZYNQ '(cd proj-arm64/sample/mm_cnn_lf;   ./MAKE_ZYNQ)'"
 (cd;sget proj-arm64/sample/mm_cnn_lf/[a-z]*zynq*)
 (cd;sget proj-arm64/sample/mm_cnn_lf/[a-z]*vmk180*)
sssh "ssh $HOST_ACAP '(cd proj-arm64/sample/mm_cnn_lf;   ./MAKE_ACAP)'"
 (cd;sget proj-arm64/sample/mm_cnn_lf/[a-z]*acap*)
 (cd;sget proj-arm64/sample/mm_cnn_lf/*obj)

#####################################################stencil
 (cd stencil;                                            ./MAKE_BSD)
 (cd;sput proj-arm64/sample/stencil/{M*,*.[ch],*obj})
sssh "ssh $HOST_CENT '(cd proj-arm64/sample/stencil;     ./MAKE_CENT)'"
 (cd;sget proj-arm64/sample/stencil/stencil-cent*)
sssh "ssh $HOST_ZYNQ '(cd proj-arm64/sample/stencil;     ./MAKE_ZYNQ)'"
 (cd;sget proj-arm64/sample/stencil/stencil-zynq*)
sssh "ssh $HOST_ACAP '(cd proj-arm64/sample/stencil;     ./MAKE_ACAP)'"
 (cd;sget proj-arm64/sample/stencil/stencil-acap*)
 (cd;sget proj-arm64/sample/stencil/*obj)

#####################################################stringsearch
 (cd stringsearch;                                       ./MAKE_BSD)
